{
  "name": "photoswiper",
  "version": "3.0.2",
  "description": "A plugin for easy and accessible PhotoSwipe initialization",
  "browser": "dist/photoswiper.browser.js",
  "main": "dist/photoswiper.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "rollup --config rollup.config.browser.js",
    "build:node": "rollup --config rollup.config.node.js",
    "dev": "parcel examples/index.html",
    "lint": "eslint src",
    "prebuild": "rm -rf dist",
    "preversion": "npm run build && git add dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh0ji/photoswiper.git"
  },
  "keywords": [
    "photoswipe",
    "accessibility",
    "a11y"
  ],
  "author": "Evan Yamanishi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sh0ji/photoswiper/issues"
  },
  "homepage": "https://github.com/sh0ji/photoswiper#readme",
  "dependencies": {
    "lodash.merge": "^4.6.1",
    "photoswipe": "^4.1.1",
    "tabtrap": "^1.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-preset-airbnb": "^3.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "rollup": "^0.67.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
}
